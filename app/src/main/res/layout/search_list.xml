<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:foo="http://schemas.android.com/apk/res/com.example.suleyman.project_a"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dialog_background"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout12">


    </RelativeLayout>

    <com.example.suleyman.project_a.TextViewPlus
        android:id="@+id/txt_qnqcode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="13dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="20dp"
        android:text="TextView" />

    <com.example.suleyman.project_a.TextViewPlus
        android:id="@+id/txt_qnqname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txt_qnqcode"
        android:layout_alignStart="@+id/txt_qnqcode"
        android:layout_below="@+id/txt_qnqcode"
        android:layout_marginTop="13dp"
        android:textColor="@color/black"
        android:text="TextView" />


</RelativeLayout>