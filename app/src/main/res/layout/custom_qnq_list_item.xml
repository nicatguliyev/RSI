<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="#00f6eef1"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/itemView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#f6eef1">

        <TextView
            android:id="@+id/codeTxt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:textColor="#000000"
            android:text="7214200"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:textStyle="bold"/>


        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/codeTxt"
            android:layout_marginLeft="10dp"
            android:id="@+id/nameTxt"
            android:textColor="#9b000000"
            android:textSize="14sp"
            android:text="Sgopa kamer necesen Ne var ne yox salamatciliqdi "/>

    </RelativeLayout>

</RelativeLayout>
