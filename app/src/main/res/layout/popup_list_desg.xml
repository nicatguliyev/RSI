<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:foo="http://schemas.android.com/apk/res/com.example.suleyman.project_a"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dialog_background"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout12">

        <com.example.suleyman.project_a.ButtonPlus
            android:id="@+id/button5"
            android:layout_width="90dp"
            android:layout_height="28dp"
            android:background="@drawable/orange_arrow_btn"
            android:textColor="@color/cpb_white"
            foo:customFont="fonts/Cera PRO.ttf"
            android:textSize="12dp"
            android:text="Button"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <RelativeLayout
            android:id="@+id/relativeLayout6"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginEnd="14dp"
            android:layout_marginRight="14dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true">

            <com.example.suleyman.project_a.TextViewPlus
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Fakt"
                android:textColor="@color/black"
                android:layout_marginLeft="10dp"
                android:layout_marginStart="10dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                foo:customFont="fonts/Cera PRO.ttf"/>
            <com.example.suleyman.project_a.TextViewPlus
                android:id="@+id/textView28"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:text="Fakt-16"
                android:layout_below="@+id/textView20"
                android:layout_alignLeft="@+id/textView20"
                android:layout_alignStart="@+id/textView20"
                foo:customFont="fonts/Cera PRO.ttf"/>
            <com.example.suleyman.project_a.TextViewPlus
                android:id="@+id/textView36"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:text="Plan"
                android:layout_below="@+id/textView28"
                android:layout_alignLeft="@+id/textView28"
                android:layout_alignStart="@+id/textView28"
                foo:customFont="fonts/Cera PRO.ttf"/>

            <com.example.suleyman.project_a.TextViewPlus
                android:id="@+id/textView38"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:text="Faiz"
                android:layout_below="@+id/textView36"
                android:layout_alignLeft="@+id/textView36"
                android:layout_alignStart="@+id/textView36"
                foo:customFont="fonts/Cera PRO.ttf"/>
            <com.example.suleyman.project_a.TextViewPlus
                android:id="@+id/txt_fakt_16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_alignBaseline="@+id/textView28"
                android:layout_alignBottom="@+id/textView28"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                foo:customFont="fonts/Cera PRO Bold.ttf"/>

            <com.example.suleyman.project_a.TextViewPlus
                android:id="@+id/txt_fakt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="TextView"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                foo:customFont="fonts/Cera PRO Bold.ttf"/>

            <com.example.suleyman.project_a.TextViewPlus
                android:id="@+id/txt_plan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="TextView"
                android:layout_alignBaseline="@+id/textView36"
                android:layout_alignBottom="@+id/textView36"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                foo:customFont="fonts/Cera PRO Bold.ttf"/>

            <com.example.suleyman.project_a.TextViewPlus
                android:id="@+id/txt_faiz"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:text="TextView"
                android:layout_alignBaseline="@+id/textView38"
                android:layout_alignBottom="@+id/textView38"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                foo:customFont="fonts/Cera PRO Bold.ttf"/>


        </RelativeLayout>


    </RelativeLayout>



</RelativeLayout>