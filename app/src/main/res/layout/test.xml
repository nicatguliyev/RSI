<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:foo="http://schemas.android.com/apk/res/com.example.suleyman.project_a"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/relativeLayout5"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <RelativeLayout
            android:layout_weight="0.1"
            android:layout_width="35dp"
            android:layout_height="fill_parent"
            android:background="@drawable/cell_"
            android:layout_alignParentTop="true"
            android:weightSum="1"
            android:orientation="vertical"
            android:id="@+id/linearLayout">
            <com.example.suleyman.project_a.TextViewPlus
                foo:customFont="fonts/Cera PRO Bold.ttf"
                android:textColor="@color/cpb_white"
                android:id="@+id/textView29"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"

                android:text="Gün"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="160dp"
            android:layout_height="fill_parent"
            android:weightSum="1"
            android:background="@drawable/cell_"
            android:orientation="vertical"
            android:id="@+id/linearLayout2"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/linearLayout"
            android:layout_toEndOf="@+id/linearLayout">


            <com.example.suleyman.project_a.TextViewPlus
                foo:customFont="fonts/Cera PRO Bold.ttf"
                android:textColor="@color/cpb_white"
                android:id="@+id/textView35"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:text="Daşıma"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="150dp"
            android:weightSum="1"
            android:layout_height="fill_parent"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/linearLayout2"
            android:layout_toRightOf="@+id/linearLayout2"
            android:orientation="vertical"
            android:background="@drawable/cell_"
            android:id="@+id/relativeLayout7">


            <com.example.suleyman.project_a.TextViewPlus
                foo:customFont="fonts/Cera PRO Bold.ttf"
                android:textColor="@color/cpb_white"
                android:id="@+id/textView45"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Müqayisə/artım"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />


        </RelativeLayout>

        <LinearLayout
            android:layout_width="10dp"
            android:layout_height="fill_parent"

            android:orientation="vertical"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_toRightOf="@+id/relativeLayout7"
            android:layout_toEndOf="@+id/relativeLayout7">

        </LinearLayout>
    </RelativeLayout>




</RelativeLayout>
